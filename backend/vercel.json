{
    "functions": {
      "api/**.js": {
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "backend/pages/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/pages/api/$1.js"
      }
    ]
  }
  